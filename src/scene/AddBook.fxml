<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddBookController">
   <children>
      <Label layoutX="260.0" layoutY="90.0" text="Adauga Carte" />
      <Label layoutX="72.0" layoutY="145.0" text="Numele cartii" />
      <Label layoutX="72.0" layoutY="183.0" text="Genul cartii" />
      <Label layoutX="72.0" layoutY="221.0" text="Nume autor" />
      <TextField fx:id="genCarte" layoutX="168.0" layoutY="179.0" promptText="gen carte" onKeyReleased="#keyReleasedProperty"/>
      <TextField fx:id="numeCarte" layoutX="168.0" layoutY="141.0" promptText="nume carte" onKeyReleased="#keyReleasedProperty"/>
      <TextField fx:id="numeAutor" layoutX="168.0" layoutY="217.0" promptText="nume autor" onKeyReleased="#keyReleasedProperty"/>
      <Button layoutX="365.0" layoutY="271.0" mnemonicParsing="false" text="Adauga" fx:id="buttonAdaugaCarte" onAction="#addBookAction"/>
      <Hyperlink layoutX="68.0" layoutY="323.0" text="Main Menu" fx:id="hyperlinkMainMenu" onAction="#goToManagerMenu"/>
   </children>
</AnchorPane>
