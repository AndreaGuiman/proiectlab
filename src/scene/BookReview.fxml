<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.BookReviewController">
   <children>
      <Label layoutX="100.0" layoutY="66.0" text="Titlu carte:" />
      <Label layoutX="123.0" layoutY="100.0" text="Autor:" />
      <TextArea fx:id="textArea" layoutX="97.0" layoutY="149.0" prefHeight="105.0" prefWidth="406.0" onKeyReleased="#keyReleasedProperty"/>
      <Button fx:id="buttonReview" layoutX="471.0" layoutY="354.0" mnemonicParsing="false" onAction="#reviewAction" text="Acorda review" />
      <TextField fx:id="rating" layoutX="140.0" layoutY="284.0" prefHeight="25.0" prefWidth="29.0" onKeyReleased="#keyReleasedProperty"/>
      <Label layoutX="97.0" layoutY="288.0" text="Nota:" />
      <Label layoutX="175.0" layoutY="288.0" text="/5" />
      <Hyperlink fx:id="hyperlinkMeniu" layoutX="41.0" layoutY="355.0" onAction="#goToMenu" text="Meniu Principal" />
      <Label fx:id="bookName" layoutX="167.0" layoutY="66.0" prefHeight="17.0" prefWidth="406.0" />
      <Label fx:id="authorName" layoutX="167.0" layoutY="96.0" prefHeight="25.0" prefWidth="406.0" />
   </children>
</AnchorPane>
