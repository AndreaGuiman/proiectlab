<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SignupController">
    <children>
        <Label layoutX="279.0" layoutY="96.0" text="Sign Up" />
        <Label layoutX="59.0" layoutY="183.0" text="Username:" />
        <Label layoutX="60.0" layoutY="221.0" text="Password:" />
        <Label layoutX="14.0" layoutY="262.0" text="Confirm password:" />
        <TextField fx:id="usernameField" layoutX="124.0" layoutY="179.0" onKeyReleased="#keyReleasedProperty" promptText="Username" />
        <PasswordField fx:id="passwordField" layoutX="124.0" layoutY="217.0" onKeyReleased="#keyReleasedProperty" promptText="Password" />
        <PasswordField fx:id="cPasswordField" layoutX="124.0" layoutY="258.0" onKeyReleased="#keyReleasedProperty" promptText="Confirm password" />
        <Label layoutX="319.0" layoutY="183.0" text="Last name:" />
        <Label layoutX="321.0" layoutY="221.0" text="Email:" />
        <Label layoutX="321.0" layoutY="262.0" text="Telephone number:" />
        <TextField fx:id="lastName" layoutX="386.0" layoutY="179.0" onKeyReleased="#keyReleasedProperty" prefHeight="25.0" prefWidth="200.0" promptText="Last name" />
        <TextField fx:id="email" layoutX="370.0" layoutY="217.0" onKeyReleased="#keyReleasedProperty" prefHeight="25.0" prefWidth="216.0" promptText="Email" />
        <TextField fx:id="telephoneNumber" layoutX="437.0" layoutY="258.0" onKeyReleased="#keyReleasedProperty" promptText="Telephone number" />
        <Button fx:id="buttonSignup" layoutX="460.0" layoutY="319.0" mnemonicParsing="false" onAction="#signUpAction" text="Sign Up" />
        <Hyperlink fx:id="hyperlinkLogin" layoutX="107.0" layoutY="320.0" onAction="#goToLogin" text="login" />
        <Label layoutX="46.0" layoutY="323.0" text="Go back to " />
        <Label layoutX="319.0" layoutY="148.0" text="First name:" />
        <TextField fx:id="firstName" layoutX="386.0" layoutY="144.0" onKeyReleased="#keyReleasedProperty" prefHeight="25.0" prefWidth="200.0" promptText="First name" />
    </children>
</AnchorPane>
